<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>HuskarWeChart</display-name>
	<welcome-file-list>
		<welcome-file>wechart</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>wechart</servlet-name>
		<servlet-class>com.huskar.wechart.WeChartMain</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>monkingtest</servlet-name>
		<servlet-class>com.huskar.wechart.MonkingTest</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>monkingtest</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>
</web-app>